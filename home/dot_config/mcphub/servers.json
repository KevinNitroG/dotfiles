{
  "mcpServers": {
    "chrome-devtools": {
      "args": [
        "-y",
        "chrome-devtools-mcp@latest"
      ],
      "command": "npx",
      "disabled": true
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [],
      "command": "npx",
      "disabled": true
    },
    "ddg-search": {
      "args": [
        "duckduckgo-mcp-server"
      ],
      "command": "uvx",
      "disabled": true
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": true
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "/home/kevinnitro"
      ],
      "autoApprove": [],
      "command": "npx",
      "disabled": true
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "/home/kevinnitro"
      ],
      "command": "uvx",
      "disabled": true
    },
    "github": {
      "disabled": true,
      "headers": {
        "Authorization": "Bearer ${GITHUB_TOKEN}"
      },
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "kubernetes": {
      "args": [
        "mcp-server-kubernetes"
      ],
      "command": "npx",
      "disabled": true
    },
    "mcp-atlassian": {
      "args": [
        "--with",
        "pydantic==2.11.9",
        "mcp-atlassian"
      ],
      "command": "uvx",
      "disabled": true,
      "env": {
        "CONFLUENCE_API_TOKEN": "${ATLASSIAN_API_KEY}",
        "CONFLUENCE_URL": "https://buggilies-se358.atlassian.net/wiki",
        "CONFLUENCE_USERNAME": "trannguyenthaibinh46@gmail.com",
        "JIRA_API_TOKEN": "${ATLASSIAN_API_KEY}",
        "JIRA_URL": "https://buggilies-se358.atlassian.net",
        "JIRA_USERNAME": "trannguyenthaibinh46@gmail.com"
      }
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx",
      "disabled": true
    },
    "playwright": {
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--config",
        "${HOME}/.config/playwright-mcp.json"
      ],
      "autoApprove": [
        "browser_close",
        "browser_resize",
        "browser_console_messages",
        "browser_handle_dialog",
        "browser_evaluate",
        "browser_file_upload",
        "browser_fill_form",
        "browser_install",
        "browser_press_key",
        "browser_type",
        "browser_navigate",
        "browser_navigate_back",
        "browser_network_requests",
        "browser_take_screenshot",
        "browser_snapshot",
        "browser_click",
        "browser_drag",
        "browser_hover",
        "browser_select_option",
        "browser_tabs",
        "browser_wait_for"
      ],
      "command": "npx",
      "disabled": false,
      "disabled_tools": [
        "browser_install"
      ],
      "env": {
        "WAYLAND_DISPLAY": "${WAYLAND_DISPLAY}",
        "XDG_RUNTIME_DIR": "${XDG_RUNTIME_DIR}"
      }
    },
    "puppeteer": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx",
      "disabled": true
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": true
    },
    "supabase": {
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "${SUPABASE_TOKEN}"
      ],
      "autoApprove": [],
      "command": "npx",
      "disabled": true
    },
    "supabase-local": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-postgres",
        "http://localhost:54323"
      ],
      "command": "npx",
      "disabled": true
    },
    "tavily": {
      "args": [
        "-y",
        "tavily-mcp@0.1.2"
      ],
      "command": "npx",
      "disabled": true,
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "time": {
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Ho_Chi_Minh"
      ],
      "command": "uvx",
      "disabled": true
    },
    "vectorcode": {
      "args": [],
      "command": "vectorcode-mcp-server",
      "disabled": true
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "autoApprove": [],
      "disabled": true,
      "disabled_prompts": [
        "create_native_server"
      ],
      "disabled_resources": [
        "mcphub://docs",
        "mcphub://native_server_guide",
        "mcphub://changelog"
      ],
      "disabled_tools": []
    },
    "neovim": {
      "autoApprove": [],
      "disabled": true,
      "disabled_prompts": [
        "parrot"
      ]
    }
  }
}
